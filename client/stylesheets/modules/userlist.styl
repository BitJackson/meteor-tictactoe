.userlist {
	margin: 30px auto;
	width: 290px;
	padding: 10px;
	border: 1px #333 solid;
}

.userlist ul {
	margin: 10px 0;
	width: 100%;
}

.userlist li {
	font-size: 2em;
	line-height: 1.8em;
	width: 100%;
	height: 50px;
	background-color: #EDEDED;
	color: #999;
	margin-bottom: 5px;
}

.userlist li a {
	margin-left: 10px;
}