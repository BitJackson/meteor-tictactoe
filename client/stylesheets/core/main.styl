body {
	font-family: 'Denk One', Arial, sans-serif;
	background: #EFEFEF;
	min-width: 320px;
	min-height: 480px;
	overflow-x: hidden;
}
.container {
	background-color: white;
	margin: 0 auto;
	width: 330px;
	height: 100%;
	padding: 10px;
}
a {
	text-decoration: none;
}
h1 {
	font-size: 2.5em;
}
h2 {
	font-size: 2em;
}
h3 {
	font-size: 1.5em;
}
li {
	margin: 5px 0;
}